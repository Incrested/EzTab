{
  "manifest_version": 2,
  "name": "EzTab",
  "description": "Simplistic, Secured New Tabs",
  "version": "1.1",
  "chrome_url_overrides": {
    "newtab" : "window.html"
  },
  "icons": {
	"128": "logo.png"
  },
  "permissions": [
	"http://*/*",
	"https://*/*",
	"activeTab",
	"storage",
	"tabs"
  ],
  "applications": {
	"gecko": {
		"id": "incrested@protonmail.ch"
	}
  }
}
